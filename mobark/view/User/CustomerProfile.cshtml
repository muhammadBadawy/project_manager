@model IEnumerable<customerApp.Models.pm_project>
@{
    ViewBag.Title = "CustomerProfile";
}


@Html.ActionLink("CreatingProjects", "Add", null, new { @class = "btn btn-primary" })
<br />
<table class="table table-striped">
    <tr>
        <th>@Html.DisplayNameFor(x => x.title)</th>
        <th>@Html.DisplayNameFor(x => x.end_date)</th>
        
        <th></th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.title)</td>
            <td>@Html.DisplayFor(m => item.end_date)</td>
            
            <td>
                @Html.ActionLink("EditProject", "EditProject", new { id = item.id }, new { @class = "btn btn-warning" })
                @Html.ActionLink("Delete", "Delete", new { id = item.id}, new { @class = "btn btn-danger" })
            </td>
        </tr>
    }

</table>
